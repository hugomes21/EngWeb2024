extends layout 

block content 
    .w3-card-4 
        header.w3-container.w3-teal 
            h1 
                span Periods List 
        .w3-container
            table.w3-table-all
                each periodo in periodos
                    tr
                        td 
                            a(href='/periodos/' + periodo)= periodo
    footer.w3-container.w3-blue
        a.w3-btn.w3-round.w3-gray(style='margin-top: 10px;' href='/') Back to Composers 
        br
        h5 Generated by Rodas in #{date}